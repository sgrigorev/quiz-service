// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Test {
  id        Int         @default(autoincrement()) @id
  name      String
  createdAt DateTime    @default(now())
  questions Question[]
}

model Question {
  id      Int     @default(autoincrement()) @id
  text    String
  theme   String?
  type    String
  answers Json
  tests   Test[]
}
